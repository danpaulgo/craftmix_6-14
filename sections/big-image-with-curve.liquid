<div id="section-{{section.id}}">
  <div class='mobile-image-section mobile-only'>
    <img src='{{ 'mobile-' | append: section.settings.color  | append: '-wave.svg' | asset_url}}' class='mobile-top-curve mobile-only' />
    <div class='mobile-image-container'>
      {{ section.settings.mobile-title }}
      {% if section.settings.use_mobile_asset_svg %}
          <object data='{{ section.settings.mobile_asset_svg | asset_url }}' ></object>
      {% endif%}
      {% for block in section.blocks %}
        <div class='mobile-block' style='width: {{ block.settings.width }}%; {% if block.settings.mobile_margin %}margin-bottom: -12px;{% endif %}'>
          {{ block.settings.data }}
        </div>
      {% endfor %}
      {% unless section.settings.hide_cta %}
      <a href='{{ section.settings.shop_url }}' class='hero-button rise centered'>
          Shop Now
      </a>
      {% endunless %}
    </div>
  </div>
  <div class='image-section desktop-only'>
      <img src='{{ section.settings.color  | append: '-curve.svg' | asset_url}}' class='top-curve' />


      <div class='big-image one'>
        {% if section.settings.use_asset_svg %}
          <object data='{{ section.settings.asset_svg | asset_url }}' data-aos='fade-up' data-aos-duration="1000" data-aos-delay='400'></object>
        {% else %}
            {{ section.settings.big_one  }}
        {% endif%}

      </div>
      <div class='big-image two'>{{ section.settings.big_two  }}</div>
