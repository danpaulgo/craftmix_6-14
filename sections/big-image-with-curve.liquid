<style>
  #section-{{section.id}} .image-section {
    width: 100%;
    {% if request.path == '/pages/social-landing' or request.path == '/pages/sampler-pack-landing' %}
    background: url({{ 'pouring-bg.jpg' | asset_url }});
    background-size: cover;
    background-position: right top;
    min-height: 100vh;
    justify-content: flex-start;
    {% else %}
    justify-content: center;
    background-color: var(--{{ section.settings.color }}-color);
    {% endif %}
	padding: 20px 8vw;
    {% if section.settings.extra_bottom %}
      padding-bottom: calc(80px + 20vw);
    {% else %}
      padding-bottom: 160px;
    {% endif %}
      
    {% if section.settings.extra_top and template == 'page.social-landing'%}
      padding-top: 10vw;
      {% elsif  section.settings.extra_top %}
       padding-top: 16vw;
      
    {% endif %}
    overflow: show;
    position: relative;
	display: flex;
    flex-direction: column;
    align-items: center;
    
  }
  
  #section-{{section.id}} .mobile-image-section {
    width: 100%;
        {% if request.path == '/pages/social-landing' or request.path == '/pages/sampler-pack-landing' %}
    background: url({{ 'pouring-bg.jpg' | asset_url }});
    background-size: auto 125%;
    background-position: center top;
    min-height: 100vh;
    justify-content: flex-start;
    {% else %}
    justify-content: center;
    background-color: var(--{{ section.settings.color }}-color);
    {% endif %}
	padding: 30px 30px;
    {% if section.settings.extra_bottom %}
      padding-bottom: calc(30px + 20vw);
    {% else %}
      padding-bottom: 30px;
    {% endif %}
      
    {% if section.settings.extra_top %}
      padding-top: calc(10px + 20vw);
    {% endif %}
    position: relative;
    overflow: show;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }
  
  .mobile-image-container {
    overflow: show;
    position: relative;
	display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin: 0 auto;
    max-width: 440px; 
  }
  
  #section-{{section.id}} .mobile-image-section .mobile-image-container > * {
    margin: 12px 0;
  }
   
  
  #section-{{section.id}} .mobile-image-section .hero-button {
    margin-top: 32px; 
  }
   
  
  #section-{{section.id}} .mobile-image-section > .mobile-top-curve {
   	position:absolute;
    top:-35px;
    height: 36px;
    margin: 0;
    left: 0;
    width: 100%;
    max-width: unset;
  }
  
  .top-curve {
   	position:absolute;
    top:-130px;
    height: 130px;
    left: 0;
    width: 100%;
    max-width: unset;
  }
  
  .big-image {
    text-align: center;
   	width: 100%;
    margin-bottom: 30px;
  }
  
  .big-image.one svg, .big-image.one object{
    height: 100%;
    max-height: 60vh;
    min-height: 300px;
  }
  
  {% if section.settings.large_image %}
  #section-{{section.id}} .big-image.one object{
   	max-height: 72vh; 
    min-height: 500px;
  }
  {% endif %}
  
  {% if request.path == '/pages/social-landing'or request.path == '/pages/sampler-pack-landing' %}
  #section-{{section.id}} .big-image.two svg{
	margin-right: 100%;
  }
  
  #section-{{section.id}} .hero-button {
    left: 11%;
    position: absolute;
	top: calc(240px + 10vw)
  }
    @media (max-width: 1023px) {
       #section-{{section.id}} .mobile-image-section .mobile-image-container svg{
/* 			margin-right: auto; */
			position: absolute;
       }

      #section-{{section.id}} .hero-button.centered {
        /*       bottom: 25%; */
        /*       top: unset; */
        /*       right: auto; */
           margin-left: auto !important;
    margin-top: -60px !important;
        display: initial;
        left: 50%;
        transform: translate(-50%, 32%);
      }
    }
  {% endif %}
  
  .big-image.two svg{
    height: 100%;
   	max-height: 45vh;
    max-width: 60vw;
    min-height: 150px;
    object-fit: contain;
    margin-right: auto;
    margin-left: auto;
  }
  
  .centered {
   	margin: 0 auto; 
  }
  
</style>


{% schema %}
  {
    "name": "Big Images with Top Curve",
    "settings": [
	    {
      "type": "url",
      "id": "shop_url",
      "label": "Shop button url"
    },
    {
      "type": "select",
      "id": "color",
      "label": "Color",
      "options": [
        {
        "group": "Color",
        "value": "pink",
        "label": "Pink"
        },
        {
        "group": "Color",
        "value": "cream",
        "label": "Cream"
        },
        {
        "group": "Color",
        "value": "purple",
        "label": "Purple"
        },
        {
        "group": "Color",
        "value": "blue",
        "label": "Blue"
        }
      ],
      "default": "pink"
    },
    {
      "type": "textarea",
      "id": "big_one",
      "label": "Big Image 1"
    },
	{
      "type": "textarea",
      "id": "big_two",
      "label": "Big Image 2"
    },
	{
	  "type": "checkbox",
	  "id": "extra_top",
