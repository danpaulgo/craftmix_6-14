	},
	{
	  "type": "checkbox",
	  "id": "extra_bottom",
	  "label": "Extra padding on bottom"
	},
	{
	  "type": "checkbox",
	  "id": "use_asset_svg",
	  "label": "Use Asset SVG",
	  "info": "Upload an SVG within the Theme's Asset folder.",
	  "default": false
	},
	{
	  "type": "checkbox",
	  "id": "large_image",
	  "label": "Use Tall First Image",
	  "default": false
	},
	{
	  "type": "text",
	  "id": "asset_svg",
	  "label": "Asset SVG Name",
	  "default": "guilt-free.svg"
	},
	{
	  "type": "checkbox",
	  "id": "use_mobile_asset_svg",
